scalar Date

type Todo {
  id: ID!
  content: String!

  user: User!
}

type User {
  id: ID!
  username: String!
  email: String!
}

type Article {
    id: ID!
    title: String!
    description: String!
    status: String!
}


type Item{
  id: ID!
  label: String!
  categoryName: String!
  createdAt: Date!
  updatedAt: Date!
  cost:  Int!
  user: User!
}