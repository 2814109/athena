type Mutation {
  createTodo(input: CreateTodo!): Todo!
  updateTodo(input: UpdateTodo!): Todo!
  createUser(input: NewUser!): User!
  createEnty(input: CreateEntryRequest!): Entry!
  createPredictCost(input: CreatePredictCost!): PredictCost!
  deletePredictCost(predictCostId: Int!): Boolean!
  createPayment(input: CreatePayment!): Payment!
  deletePayment(paymentId: Int!): Boolean!
}
